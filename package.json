{
	"name": "@wajeht/project",
	"version": "0.0.1",
	"description": "project scaffolder",
	"main": "./dist/src/index.js",
	"bin": {
		"project": "./dist/src/index.js"
	},
	"scripts": {
		"postinstall": "chmod +x ./dist/src/scripts/post-install.js && ./dist/src/scripts/post-install.js",
		"cli": "npm unlink cdns && rm -rf ./dist && npm run build && chmod +x ./dist/src/index.js && npm link",
		"dev": "npm run cli && tsc -w",
		"build": "tsc --noImplicitAny",
		"format:check": "prettier --check .",
		"format": "prettier --write .",
		"lint:check": "eslint .",
		"lint": "eslint --fix .",
		"test:watch": "vitest",
		"test": "vitest --run"
	},
	"keywords": [],
	"author": "@wajeht",
	"publishConfig": {
		"registry": "https://registry.npmjs.org/",
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/wajeht/project.git"
	},
	"bugs": {
		"url": "https://github.com/wajeht/project/issues"
	},
	"license": "MIT",
	"dependencies": {
		"typescript": "^5.3.3"
	},
	"devDependencies": {
		"@types/node": "^20.11.24",
		"@typescript-eslint/eslint-plugin": "^7.1.0",
		"@typescript-eslint/parser": "^7.1.0",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.1.3",
		"prettier": "^3.2.5"
	}
}
